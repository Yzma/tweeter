{
  "name": "tweeter",
  "version": "1.0.0",
  "description": "Tweeter is a simple, single-page Twitter clone that allows the user to create tweets and view them in a compact but clean UI",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "local": "nodemon --watch server -e js server/index.js",
    "build-css": "sass sass:public/styles --no-source-map",
    "sass-watch": "sass sass:public/styles --watch --poll --no-source-map"
  },
  "author": "Andrew Caruso",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "chance": "^1.0.2",
    "express": "^4.13.4",
    "md5": "^2.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.9.2"
  }
}
